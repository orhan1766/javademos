package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {

		int number = 25;

		for (int i = 2; i < number; i++) {

			int remainder = number % i;
			
			if (remainder == 0) {
				System.out.println("Sayı asal değil.");
			} else {
				System.out.println("Sayı asal.");
			}

		}

	}

}
