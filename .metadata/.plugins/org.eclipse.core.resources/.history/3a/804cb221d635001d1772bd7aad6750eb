package recapDemo1;

public class Main {

	public static void main(String[] args) {
		int sayi1 = 3;
		int sayi2 = 25;
		int sayi3 = 27;

		if (sayi1 > sayi2) {
			if (sayi1 > sayi3) {
				System.out.println("En büyük sayı :" + sayi1);
			}
		} else if (sayi2 > sayi3) {
			if (sayi2 > sayi1) {
				System.out.println("En büyük sayı :" + sayi2);
			}
		} else if (sayi3 > sayi2) {
			if (sayi3 > sayi1) {
				System.out.println("En büyük sayı :" + sayi3);
			}
		}

	}

}
